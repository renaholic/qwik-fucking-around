{
  "name": "qwik-fucking-around",
  "description": "Recommended for your first Qwik app",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssg src/entry.preview.tsx",
    "build.server": "vite build -c adaptors/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "wrangler pages dev ./dist",
    "dev": "vite --mode ssg --host",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssg --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "prepare": "husky install",
    "preview": "qwik build preview && vite preview --open",
    "qwik-speak-extract": "qwik-speak-extract --supportedLangs=en-US,zh-HK",
    "serve": "node server/entry.express",
    "start": "vite --open --mode ssg",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.7.0",
    "@builder.io/qwik": "0.16.2",
    "@builder.io/qwik-city": "0.0.128",
    "@builder.io/qwik-react": "0.2.1",
    "@types/eslint": "8.4.10",
    "@types/express": "4.17.15",
    "@types/node": "latest",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.47.0",
    "autoprefixer": "^10.4.12",
    "eslint": "8.30.0",
    "eslint-plugin-qwik": "0.16.1",
    "express": "4.18.2",
    "husky": "^8.0.0",
    "node-fetch": "3.3.0",
    "postcss": "^8.4.16",
    "prettier": "2.8.2",
    "pretty-quick": "^3.1.3",
    "qwik-speak": "^0.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.1.8",
    "typescript": "4.9.4",
    "vite": "3.2.4",
    "vite-plugin-pwa": "^0.14.0",
    "vite-tsconfig-paths": "3.6.0",
    "wrangler": "latest"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.7",
    "@tailwindcss/forms": "^0.5.3",
    "@types/youtube-player": "^5.5.6",
    "dayjs": "^1.11.7",
    "firebase": "^9.15.0",
    "focus-visible": "^5.2.0",
    "postcss-focus-visible": "^7.1.0",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "tailwind-merge": "^1.8.1",
    "typed.js": "^2.0.12",
    "youtube-player": "^5.5.2"
  }
}
